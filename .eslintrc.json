{
	"root": true,
	"extends": [
		"wikimedia/server"
	],
	"rules": {
		"array-bracket-spacing": "off",
		"camelcase": "off",
		"computed-property-spacing": "off",
		"indent": [ "off" ],
		"no-console": "off",
		"no-process-exit": "off",
		"no-shadow": "off",
		"no-underscore-dangle": "off",
		"no-use-before-define": "off",
		"space-in-parens": "off",
		"jsdoc/newline-after-description": "off"
	}
}
